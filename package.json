{
  "name": "lotion-chain",
  "version": "0.0.1",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "build:dev": "babel src -w -x '.ts' -d build",
    "app": "node build/app.js",
    "serve": "nodemon build/app.js",
    "start": "run-p build:dev serve",
    "lint": "run-p tscheck tslint",
    "tscheck": "tsc --project ./tsconfig.json --noEmit",
    "tslint": "tslint --config ./tslint.json './src/**/*.ts'",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --config ./tslint.json './src/**/*.ts' --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keyiiiii/lotion-chain.git"
  },
  "author": "keyiiiii",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keyiiiii/lotion-chain/issues"
  },
  "homepage": "https://github.com/keyiiiii/lotion-chain#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "lotion": "^0.1.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-syntax-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "babel-preset-env": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  }
}
